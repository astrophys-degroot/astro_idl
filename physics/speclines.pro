pro speclines, which, ZZ=zz, VALS=vals

  IF keyword_set(ZZ) THEN zz = zz[0] ELSE zz = 0.0 
  CASE which OF
     'yband_mosfire' : vals = ysky_mosfire(zz)
     'yband_medium' : vals = ysky_medium(zz)
     'yband_faint' : vals = ysky_faint(zz)
     'jband_mosfire' : vals = jsky_mosfire(zz)
     'jband_medium' : vals = jsky_medium(zz)
     'jband_faint' : vals = jsky_faint(zz)
     'hband_mosfire' : vals = hsky_mosfire(zz)
     'hband_bright' : vals = hsky_bright(zz)
