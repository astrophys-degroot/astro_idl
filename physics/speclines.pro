     'kband_faint' : vals = ksky_faint(zz)
     'balmer' : vals = balmer(zz)
     'nonbalmer' : vals = nonbalmer(zz)
     'uvlines' : vals = uvlines(zz)
     'breaks' : vals = breaks(zz)
     ELSE : BEGIN
        print, 'Spectral line request not found!! Please try another from the following...'
        print, '  yband_mosfire, yband_medium, yband_faint, jband_mosfire, jband_medium, jband_faint, hband_mosfire, ' + $
               '  hband_bright, hband_faint, kband, balmer, nonbalmer, uvlines, breaks ...'
        stop
     ENDELSE
  ENDCASE
      
  RETURN
END
